<script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script>
 google.load("visualization", "1", {packages: ["corechart"]});

        google.setOnLoadCallback(drawChart);
function drawChart() {
    var discreteData = new google.visualization.DataTable();
    discreteData.addColumn('string', 'Number');
    discreteData.addColumn('number', 'Value');

    discreteData.addRows({{=str(meandata)}});

    var discreteData1 = new google.visualization.DataTable();
    discreteData1.addColumn('string', 'Number');
    discreteData1.addColumn('number', 'Value');

    discreteData1.addRows({{=str(maxdata)}});

    var discreteData2 = new google.visualization.DataTable();
    discreteData2.addColumn('string', 'Number');
    discreteData2.addColumn('number', 'Value');

    discreteData2.addRows({{=str(mindata)}});

    var discreteData3 = new google.visualization.DataTable();
    discreteData3.addColumn('string', 'Number');
    discreteData3.addColumn('number', 'Value');

    discreteData3.addRows({{=str(meddata)}});

    var discreteData4 = new google.visualization.DataTable();
    discreteData4.addColumn('string', 'Number');
    discreteData4.addColumn('number', 'Value');

    discreteData4.addRows({{=str(devdata)}});



    var discreteChart = new google.visualization.LineChart(document.getElementById('chart_div'));
    discreteChart.draw(discreteData, {

        vAxis: {
          title: 'Mean {{=s}} values'
        }
      });

    var discreteChart1 = new google.visualization.LineChart(document.getElementById('chart_div1'));
    discreteChart1.draw(discreteData1, {

        vAxis: {
          title: 'Maximum {{=s}} values'
        }
      });


    var discreteChart2 = new google.visualization.LineChart(document.getElementById('chart_div3'));
    discreteChart2.draw(discreteData2, {

        vAxis: {
          title: 'Maximum {{=s}} values'
        }
      });

    var discreteChart3 = new google.visualization.LineChart(document.getElementById('chart_div4'));
    discreteChart3.draw(discreteData3, {

        vAxis: {
          title: 'Maximum {{=s}} values'
        }
      });

    var discreteChart4 = new google.visualization.LineChart(document.getElementById('chart_div5'));
    discreteChart4.draw(discreteData4, {

        vAxis: {
          title: 'Maximum {{=s}} values'
        }
      });


}

</script>

